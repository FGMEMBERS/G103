        <object-name>aileronR1</object-name>
        <object-name>aileronR2</object-name>
        <object-name>flapL1</object-name>
        <object-name>flapL2</object-name>
        <object-name>flapR1</object-name>
        <object-name>flapR2</object-name>
        <object-name>spoilerL</object-name>
        <object-name>spoilerR</object-name>
        <object-name>vstab</object-name>
        <object-name>elevator</object-name>
        <object-name>hstab</object-name>
        <object-name>rudder</object-name>
        <property-base>sim/model/livery</property-base>
        <texture-prop>texture</texture-prop>
        <texture>texture.png</texture>
    </animation>
</PropertyList>
