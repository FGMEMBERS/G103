<?xml version="1.0"?>
<!-- Schleicher ASK 21 Glider by Heiko Schulz -->
<PropertyList>
    <path>g103.ac</path>
    <nasal>
        <load>
            var livery_update = aircraft.livery_update.new("Aircraft/ASK21/Models/Liveries", 10);

            var float_0 = self.getNode("sim/multiplay/generic/float[0]", 1);
            var float_1 = self.getNode("sim/multiplay/generic/float[1]", 1);

            var canopyF_door = self.getNode("sim/model/door-positions/canopyF/position-norm", 1);
            var canopyB_door = self.getNode("sim/model/door-positions/canopyB/position-norm", 1);

            canopyF_door.alias(float_0);
            canopyB_door.alias(float_1);
        </load>
        <unload>
            livery_update.stop();
            canopyF_door.unalias();
            canopyB_door.unalias();
        </unload>
    </nasal>
    <!--WING BENDING-->
    <animation>
        <type>rotate</type>
        <object-name>wing.L1</object-name>
        <object-name>wing.L2</object-name>
        <object-name>wing.L3</object-name>
        <object-name>wing.L4</object-name>
        <object-name>aileronL1</object-name>
        <object-name>aileronL2</object-name>
        <object-name>spoilerL</object-name>
        <property>accelerations/pilot-g[0]</property>
        <factor>-1</factor>
        <offset-deg>1</offset-deg>
        <center>
            <x-m> 0.00</x-m>
            <y-m>-0.2813</y-m>
            <z-m>-0.34366</z-m>
        </center>
        <axis>
            <x>1.0</x>
            <y>0.0</y>
            <z>0.0</z>
        </axis>
    </animation>
    <animation>
        <type>rotate</type>
        <object-name>wing.L2</object-name>
        <object-name>wing.L3</object-name>
        <object-name>wing.L4</object-name>
        <object-name>aileronL1</object-name>
        <object-name>aileronL2</object-name>
        <object-name>spoilerL</object-name>
        <property>accelerations/pilot-g[0]</property>
        <factor>-1</factor>
        <offset-deg>1</offset-deg>
        <center>
            <x-m> 0.00</x-m>
            <y-m>-2.31877</y-m>
            <z-m>-0.21581</z-m>
        </center>
        <axis>
            <x>1.0</x>
            <y>0.0</y>
            <z>0.0</z>
        </axis>
    </animation>
    <animation>
        <type>rotate</type>
        <object-name>wing.L3</object-name>
        <object-name>wing.L4</object-name>
        <object-name>aileronL1</object-name>
        <object-name>aileronL2</object-name>
        <property>accelerations/pilot-g[0]</property>
        <factor>-1</factor>
        <offset-deg>1</offset-deg>
        <center>
            <x-m>  0.00</x-m>
            <y-m>-4.87214</y-m>
            <z-m> -0.04684</z-m>
        </center>
        <axis>
            <x>1.0</x>
            <y>0.0</y>
            <z>0.0</z>
        </axis>
    </animation>
    <animation>
        <type>rotate</type>
        <object-name>wing.L4</object-name>
        <object-name>aileronL2</object-name>
        <property>accelerations/pilot-g[0]</property>
        <factor>-1</factor>
        <offset-deg>1</offset-deg>
        <center>
            <x-m>  0.00</x-m>
            <y-m>-6.68827</y-m>
            <z-m>0.08792</z-m>
        </center>
        <axis>
            <x>1.0</x>
            <y>0.0</y>
            <z>0.0</z>
        </axis>
    </animation>
    <animation>
        <type>rotate</type>
        <object-name>wing.R1</object-name>
        <object-name>wing.R2</object-name>
        <object-name>wing.R3</object-name>
        <object-name>wing.R4</object-name>
        <object-name>aileronR1</object-name>
        <object-name>aileronR2</object-name>
        <object-name>spoilerR</object-name>
        <property>accelerations/pilot-g[0]</property>
        <factor>1</factor>
        <offset-deg>-1</offset-deg>
        <center>
            <x-m> 0.00</x-m>
            <y-m>0.2813</y-m>
            <z-m>-0.34366</z-m>
        </center>
        <axis>
            <x>1.0</x>
            <y>0.0</y>
            <z>0.0</z>
        </axis>
    </animation>
    <animation>
        <type>rotate</type>
        <object-name>wing.R2</object-name>
        <object-name>wing.R3</object-name>
        <object-name>wing.R4</object-name>
        <object-name>aileronR1</object-name>
        <object-name>aileronR2</object-name>
        <object-name>spoilerR</object-name>
        <property>accelerations/pilot-g[0]</property>
        <factor>1</factor>
        <offset-deg>-1</offset-deg>
        <center>
            <x-m> 0.00</x-m>
            <y-m>2.31877</y-m>
            <z-m>-0.21581</z-m>
        </center>
        <axis>
            <x>1.0</x>
            <y>0.0</y>
            <z>0.0</z>
        </axis>
    </animation>
    <animation>
        <type>rotate</type>
        <object-name>wing.R3</object-name>
        <object-name>wing.R4</object-name>
        <object-name>aileronR1</object-name>
        <object-name>aileronR2</object-name>
        <property>accelerations/pilot-g[0]</property>
        <factor>1</factor>
        <offset-deg>-1</offset-deg>
        <center>
            <x-m>  0.00</x-m>
            <y-m>4.87214</y-m>
            <z-m> -0.04684</z-m>
        </center>
        <axis>
            <x>1.0</x>
            <y>0.0</y>
            <z>0.0</z>
        </axis>
    </animation>
    <animation>
        <type>rotate</type>
        <object-name>wing.R4</object-name>
        <object-name>aileronR2</object-name>
        <property>accelerations/pilot-g[0]</property>
        <factor>1</factor>
        <offset-deg>-1</offset-deg>
        <center>
            <x-m>  0.00</x-m>
            <y-m>6.68827</y-m>
            <z-m>0.08792</z-m>
        </center>
        <axis>
            <x>1.0</x>
            <y>0.0</y>
            <z>0.0</z>
        </axis>
    </animation>
    <!--FLIGHT CONTROL SURFACES-->
    <animation>
        <type>rotate</type>
        <object-name>aileronL1</object-name>
        <object-name>aileronL2</object-name>
        <property>controls/flight/aileron</property>
        <factor>-24</factor>
        <axis>
            <x1-m>-0.69082</x1-m>
            <y1-m>-4.92347</y1-m>
            <z1-m>-0.03724</z1-m>
            <x2-m>-1.01657</x2-m>
            <y2-m>-8.41999</y2-m>
            <z2-m>0.1983</z2-m>
        </axis>
    </animation>
    <animation>
        <type>rotate</type>
        <object-name>aileronR1</object-name>
        <object-name>aileronR2</object-name>
        <property>controls/flight/aileron</property>
        <factor>-24</factor>
        <axis>
            <x1-m>-0.69082</x1-m>
            <y1-m>4.92347</y1-m>
            <z1-m>-0.03724</z1-m>
            <x2-m>-1.01657</x2-m>
            <y2-m>8.41999</y2-m>
            <z2-m>0.1983</z2-m>
        </axis>
    </animation>
    <!--airbrakes-->
    <animation>
        <type>translate</type>
        <object-name>spoilerL</object-name>
        <object-name>spoilerR</object-name>
        <property>/surface-positions/speedbrake-pos-norm</property>
        <factor>0.07</factor>
        <axis>
            <x>0</x>
            <y>0</y>
            <z>1</z>
        </axis>
    </animation>
    <animation>
        <type>rotate</type>
        <object-name>rudder</object-name>
        <property>controls/flight/rudder</property>
        <factor>24</factor>
        <axis>
            <x1-m>3.38865</x1-m>
            <y1-m>0.0</y1-m>
            <z1-m>-0.46265</z1-m>
            <x2-m>3.73329</x2-m>
            <y2-m>0.0</y2-m>
            <z2-m>0.91188</z2-m>
        </axis>
    </animation>
    <animation>
        <type>rotate</type>
        <object-name>elevator</object-name>
        <property>controls/flight/elevator</property>
        <factor>24</factor>
        <center>
            <x-m>3.65039</x-m>
            <y-m>0.0</y-m>
            <z-m>0.91241</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>
    <!--COCKPIT-->
    <model>
        <path>Interior/interior.xml</path>
    </model>
    <!--GEAR-->
    <!--Compression-->
    <animation>
        <type>translate</type>
        <object-name>wheelF</object-name>
        <property>gear/gear/compression-norm</property>
        <factor>0.05</factor>
        <axis>
            <z>1</z>
        </axis>
    </animation>
    <animation>
        <type>translate</type>
        <object-name>wheelM</object-name>
        <property>gear/gear[1]/compression-norm</property>
        <factor>0.08</factor>
        <axis>
            <z>1</z>
        </axis>
    </animation>
    <animation>
        <type>spin</type>
        <object-name>wheelM</object-name>
        <property>/gear/gear[1]/rollspeed-ms</property>
        <factor>-25</factor>
        <axis><y>1</y></axis>
            <center>
                <x-m>-1.08408</x-m>
                <z-m>-0.75519</z-m>
            </center>
    </animation>
    <animation>
        <type>translate</type>
        <object-name>wheelR</object-name>
        <property>gear/gear[2]/compression-norm</property>
        <factor>0.05</factor>
        <axis>
            <z>1</z>
        </axis>
    </animation>
    <!--CANOPY-->
    <animation>
        <type>pick</type>
        <object-name>canopyF.glass.inside</object-name>
        <object-name>canopyF.glass.outside</object-name>
        <object-name>canopyF</object-name>
        <visible>true</visible>
        <action>
            <button>0</button>
            <repeatable>false</repeatable>
            <binding>
                <command>nasal</command>
                <script>doors.canopyF.toggle();</script>
            </binding>
        </action>
    </animation>
    <animation>
        <type>rotate</type>
        <object-name>canopyF.glass.inside</object-name>
        <object-name>canopyF.glass.outside</object-name>
        <object-name>canopyF</object-name>
        <property>sim/model/door-positions/canopyF/position-norm</property>
        <interpolation>
            <entry>
                <ind>0.0</ind>
                <dep>0</dep>
            </entry>
            <entry>
                <ind>0.1</ind>
                <dep>-1</dep>
            </entry>
            <entry>
                <ind>1</ind>
                <dep>-80</dep>
            </entry>
        </interpolation>
        <axis>
            <x1-m>-3.25048</x1-m>
            <y1-m>0.24273</y1-m>
            <z1-m>-0.3963</z1-m>
            <x2-m>-2.33396</x2-m>
            <y2-m>0.34102</y2-m>
            <z2-m>-0.34421</z2-m>
        </axis>
    </animation>
    <animation>
        <type>pick</type>
        <object-name>canopyR.glass.inside</object-name>
        <object-name>canopyR.glass.outside</object-name>
        <object-name>canopyR</object-name>
        <visible>true</visible>
        <action>
            <button>0</button>
            <repeatable>false</repeatable>
            <binding>
                <command>nasal</command>
                <script>doors.canopyB.toggle();</script>
            </binding>
        </action>
    </animation>
    <animation>
        <type>rotate</type>
        <object-name>canopyR.glass.inside</object-name>
        <object-name>canopyR.glass.outside</object-name>
        <object-name>canopyR</object-name>
        <property>sim/model/door-positions/canopyB/position-norm</property>
        <interpolation>
            <entry>
                <ind>0.0</ind>
                <dep>0</dep>
            </entry>
            <entry>
                <ind>0.2</ind>
                <dep>0</dep>
            </entry>
            <entry>
                <ind>1</ind>
                <dep>-80</dep>
            </entry>
        </interpolation>
        <axis>
            <x1-m>-1.99665</x1-m>
            <y1-m>0.34751</y1-m>
            <z1-m>-0.29497</z1-m>
            <x2-m>-1.55962</x2-m>
            <y2-m>0.34066</y2-m>
            <z2-m>-0.25782</z2-m>
        </axis>
    </animation>
    <!--SHADERS/EFFECTS-->
    <!--Glass shader-->
    <effect>
        <inherits-from>Effects/model-transparent</inherits-from>
        <object-name>canopyF.glass.inside</object-name>
        <object-name>canopyF.glass.outside</object-name>
        <object-name>canopyR.glass.inside</object-name>
        <object-name>canopyR.glass.outside</object-name>
    </effect>
    <!--Fuselage shader-->
    <effect>
        <inherits-from>Aircraft/ASK21/Models/Effects/ask21_wing</inherits-from>
        <object-name>wing.L1</object-name>
        <object-name>wing.L2</object-name>
        <object-name>wing.L3</object-name>
        <object-name>wing.L4</object-name>
        <object-name>wing.L5</object-name>
        <object-name>wing.R1</object-name>
        <object-name>wing.R2</object-name>
        <object-name>wing.R3</object-name>
        <object-name>wing.R4</object-name>
        <object-name>wing.R5</object-name>
        <object-name>aileronL1</object-name>
        <object-name>aileronL2</object-name>
        <object-name>aileronR1</object-name>
        <object-name>aileronR2</object-name>
        <object-name>flapL1</object-name>
        <object-name>flapL2</object-name>
        <object-name>flapR1</object-name>
        <object-name>flapR2</object-name>
        <object-name>spoilerL</object-name>
        <object-name>spoilerR</object-name>
        <object-name>vstab</object-name>
        <object-name>elevator</object-name>
        <object-name>hstab</object-name>
        <object-name>rudder</object-name>
    </effect>
    <effect>
        <inherits-from>Aircraft/ASK21/Models/Effects/ask21_model</inherits-from>
        <object-name>body</object-name>
        <object-name>canopyF</object-name>
        <object-name>canopyR</object-name>
        <object-name>gearFfairing</object-name>
        <object-name>gearMfairing</object-name>
        <object-name>gearRfairing</object-name>
    </effect>
    <!--LIVERY-->
    <animation>
        <type>material</type>
        <object-name>body</object-name>
        <object-name>canopy</object-name>
        <object-name>geardoorL</object-name>
        <object-name>geardoorR</object-name>
        <object-name>gearfairing1</object-name>
        <object-name>gearfairing2</object-name>
        <object-name>gearfairing3</object-name>
        <object-name>wing.L1</object-name>
        <object-name>wing.L2</object-name>
        <object-name>wing.L3</object-name>
        <object-name>wing.L4</object-name>
        <object-name>wing.L5</object-name>
        <object-name>wing.R1</object-name>
        <object-name>wing.R2</object-name>
        <object-name>wing.R3</object-name>
        <object-name>wing.R4</object-name>
        <object-name>wing.R5</object-name>
        <object-name>aileronL1</object-name>
