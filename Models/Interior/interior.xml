<?xml version="1.0"?>
<PropertyList>
    <path>interior.ac</path>
    <!--COCKPIT-->
    <!-- Yaw string -->
    <model>
        <name>yawstring</name>
        <path>Panel/Instruments/wool-thread/wool-thread.xml</path>
        <offsets>
            <x-m>-3.53354</x-m>
            <y-m> 0</y-m>
            <z-m>0.06949</z-m>
            <pitch-deg>0</pitch-deg>
        </offsets>
    </model>
    <model>
        <name>panelF</name>
        <path>Panel/panelF.xml</path>
    </model>
    <model>
        <name>panelR</name>
        <path>Panel/panelR.xml</path>
    </model>
    <!--Pilots-->
    <model>
        <name>pilots</name>
        <path>Pilots/pilots.xml</path>
    </model>
    <!--Small windows-->
    <animation>
        <type>slider</type>
        <object-name>sliding_window.front</object-name>
        <visible>true</visible>
        <action>
            <button>0</button>
            <repeatable>false</repeatable>
            <binding>
                <command>property-adjust</command>
                <property>sim/model/door-positions/frontwin/position-norm</property>
                <factor>-0.05</factor>
                <min>0</min>
                <max>1</max>
            </binding>
        </action>
    </animation>
    <animation>
        <type>translate</type>
        <object-name>sliding_window.front</object-name>
        <property>sim/model/door-positions/frontwin/position-norm</property>
        <interpolation>
            <entry>
                <ind>0</ind>
                <dep>0</dep>
            </entry>
            <entry>
                <ind>0.1</ind>
                <dep>0.01</dep>
            </entry>
            <entry>
                <ind>1</ind>
                <dep>0.01</dep>
            </entry>
        </interpolation>
        <axis>
            <y>1</y>
        </axis>
    </animation>
    <animation>
        <type>translate</type>
        <object-name>sliding_window.front</object-name>
        <property>sim/model/door-positions/frontwin/position-norm</property>
        <interpolation>
            <entry>
                <ind>0</ind>
                <dep>0</dep>
            </entry>
            <entry>
                <ind>0.1</ind>
                <dep>0.0</dep>
            </entry>
            <entry>
                <ind>1</ind>
                <dep>0.18</dep>
            </entry>
        </interpolation>
        <axis>
            <x1-m>-3.10267</x1-m>
            <y1-m>-0.28405</y1-m>
            <z1-m>-0.14893</z1-m>
            <x2-m>-2.76013</x2-m>
            <y2-m>-0.31171</y2-m>
            <z2-m>-0.12215</z2-m>
        </axis>
    </animation>
    <animation>
        <type>slider</type>
        <object-name>sliding_window.back</object-name>
        <visible>true</visible>
        <action>
            <button>0</button>
            <repeatable>false</repeatable>
            <binding>
                <command>property-adjust</command>
                <property>sim/model/door-positions/rearwin/position-norm</property>
                <factor>-0.05</factor>
                <min>0</min>
                <max>1</max>
            </binding>
        </action>
    </animation>
    <animation>
        <type>translate</type>
        <object-name>sliding_window.back</object-name>
        <property>sim/model/door-positions/rearwin/position-norm</property>
        <interpolation>
            <entry>
                <ind>0</ind>
                <dep>0</dep>
            </entry>
            <entry>
                <ind>0.1</ind>
                <dep>0.01</dep>
            </entry>
            <entry>
                <ind>1</ind>
                <dep>0.01</dep>
            </entry>
        </interpolation>
        <axis>
            <y>1</y>
        </axis>
    </animation>
    <animation>
        <type>translate</type>
        <object-name>sliding_window.back</object-name>
        <property>sim/model/door-positions/rearwin/position-norm</property>
        <interpolation>
            <entry>
                <ind>0</ind>
                <dep>0</dep>
            </entry>
            <entry>
                <ind>0.1</ind>
                <dep>0.0</dep>
            </entry>
            <entry>
                <ind>1</ind>
                <dep>0.17</dep>
            </entry>
        </interpolation>
        <axis>
            <x1-m>-2.1206</x1-m>
            <y1-m>-0.33096</y1-m>
            <z1-m>-0.05436</z1-m>
            <x2-m>-1.77808</x2-m>
            <y2-m>-0.32852</y2-m>
            <z2-m>-0.04506</z2-m>
        </axis>
    </animation>
    <!--airbrake grip-->
    <animation>
        <type>slider</type>
        <object-name>grip.airbrake</object-name>
        <visible>true</visible>
        <action>
            <button>0</button>
            <repeatable>false</repeatable>
            <binding>
                <command>property-adjust</command>
                <property>/controls/engines/engine/throttle</property>
                <factor>0.05</factor>
                <min>0</min>
                <max>1</max>
            </binding>
        </action>
    </animation>
    <animation>
        <type>translate</type>
        <object-name>grip.airbrake</object-name>
        <property>/controls/engines/engine/throttle</property>
        <factor>1</factor>
        <interpolation>
            <entry>
                <ind>1</ind>
                <dep>0</dep>
            </entry>
            <entry>
                <ind>0</ind>
                <dep>.25</dep>
            </entry>
        </interpolation>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>
    <animation>
        <type>translate</type>
        <object-name>grip.airbrake</object-name>
        <property>/controls/engines/engine/throttle</property>
        <factor>-1</factor>
        <interpolation>
            <entry>
                <ind>1</ind>
                <dep>0</dep>
            </entry>
            <entry>
                <ind>0</ind>
                <dep>-0.025</dep>
            </entry>
        </interpolation>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>
    <animation>
        <type>translate</type>
        <object-name>grip.airbrake</object-name>
        <property>/controls/engines/engine/throttle</property>
        <factor>1</factor>
        <interpolation>
            <entry>
                <ind>1</ind>
                <dep>0</dep>
            </entry>
            <entry>
                <ind>0</ind>
                <dep>.045</dep>
            </entry>
        </interpolation>
        <axis>
            <x>0</x>
            <y>0</y>
            <z>1</z>
        </axis>
    </animation>
    <!--stick front-->
    <animation>
        <type>rotate</type>
        <object-name>stick-front</object-name>
        <property>/controls/flight/elevator</property>
        <factor>10</factor>
        <offset-deg>0</offset-deg>
        <center>
            <x-m>-3.17203</x-m>
            <y-m>0.0</y-m>
            <z-m>0.32481</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>-1</y>
            <z>0</z>
        </axis>
    </animation>
    <animation>
        <type>rotate</type>
        <object-name>stick-front</object-name>
        <property>/controls/flight/aileron</property>
        <factor>10</factor>
        <offset>0</offset>
        <center>
            <x-m>-3.17203</x-m>
            <y-m>0.0</y-m>
            <z-m>0.32481</z-m>
        </center>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>
    <!--stick rear-->
    <animation>
        <type>rotate</type>
        <object-name>stick-rear</object-name>
        <property>/controls/flight/elevator</property>
        <factor>10</factor>
        <offset-deg>0</offset-deg>
        <center>
            <x-m>-1.99529</x-m>
            <y-m>0.0</y-m>
            <z-m>0.38177</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>-1</y>
            <z>0</z>
        </axis>
    </animation>
    <animation>
        <type>rotate</type>
        <object-name>stick-rear</object-name>
        <property>/controls/flight/aileron</property>
        <factor>10</factor>
        <offset>0</offset>
        <center>
            <x-m>-1.99529</x-m>
            <y-m>0.0</y-m>
            <z-m>0.38177</z-m>
        </center>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>
    <!--Cockpit Shader-->
    <effect>
        <inherits-from>../Effects/interior-shadow</inherits-from>
        <object-name>interiorF</object-name>
        <object-name>interiorR</object-name>
        <object-name>seatF</object-name>
        <object-name>seatR</object-name>
    </effect>
    <!--CANOPY-->
    <animation>
        <type>rotate</type>
        <object-name>yawstring</object-name>
        <object-name>window.int</object-name>
        <object-name>sliding_window.front</object-name>
        <object-name>sliding_window.back</object-name>
        <property>sim/model/door-positions/canopyF/position-norm</property>
        <interpolation>
            <entry>
                <ind>0.0</ind>
                <dep>0</dep>
            </entry>
            <entry>
                <ind>0.1</ind>
                <dep>-1</dep>
            </entry>
            <entry>
                <ind>1</ind>
                <dep>-80</dep>
            </entry>
        </interpolation>
        <axis>
            <x1-m>-3.27191</x1-m>
            <y1-m>0.28064</y1-m>
            <z1-m>-0.24765</z1-m>
            <x2-m>-1.84713</x2-m>
            <y2-m>0.3468</y2-m>
            <z2-m>-0.14673</z2-m>
        </axis>
    </animation><!--
